{
  "name": "typescript-graphql",
  "scripts": {
    "start": "ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "seed:new": "ts-node prisma/seed/seed.ts",
    "seed:add": "ts-node prisma/seed/seedProjects.ts",
    "seed:firebase": "ts-node prisma/seed/seedFirebase.ts",
    "test": "ts-mocha -r dotenv/config -p tsconfig.json --watch-extensions ts --timeout 60000 -w src/**/*.test.ts",
    "update-types": "graphql-codegen",
    "test:no-network": "ts-mocha -r dotenv/config -p tsconfig.json --watch-extensions ts --timeout 3000 --grep /network/i --invert -w src/**/*.test.ts",
    "postinstall": "run-s prisma:generate",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start:test-server": "ts-node-dev --no-notify --respawn --transpileOnly ./src/test/server.ts",
    "prisma:generate": "prisma2 generate",
    "new-db": "prisma2 lift up",
    "new-migration": "prisma2 lift save -c -n init",
    "seed:init": "run-s new-migration new-db prisma:generate seed:new",
    "clean": "rm -rf dist",
    "tsc": "tsc",
    "build": "run-s clean tsc",
    "gcs:build": "gcloud builds submit --tag gcr.io/echtd-253717/server",
    "gcs:deploy": "gcloud beta run deploy server --image gcr.io/echtd-253717/server --platform managed",
    "gcs": "run-s gcs:build gcs:deploy"
  },
  "dependencies": {
    "@google-cloud/storage": "^3.3.1",
    "@hapi/joi": "^16.1.7",
    "@sendgrid/mail": "^6.4.0",
    "apollo-server": "^2.9.6",
    "dotenv": "^8.1.0",
    "firebase-admin": "^8.6.0",
    "graphql": "^14.5.8",
    "graphql-shield": "^6.1.0",
    "graphql-upload": "^8.1.0",
    "graphql-yoga": "1.18.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nexus": "^0.12.0-beta.14",
    "nexus-prisma": "^0.5.0",
    "shortid": "^2.2.15",
    "slugify": "^1.3.5",
    "stripe": "^7.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@types/chai": "^4.2.3",
    "@types/faker": "^4.1.6",
    "@types/graphql-upload": "^8.0.3",
    "@types/hapi__joi": "^16.0.1",
    "@types/lodash": "^4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/node": "10.14.21",
    "@types/shortid": "0.0.29",
    "@types/sinon": "^7.5.0",
    "@types/stripe": "^6.32.12",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "apollo-server-testing": "^2.9.6",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "graphql-tag": "^2.10.1",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "prisma2": "^2.0.0-preview015",
    "sinon": "^7.5.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "8.4.1",
    "ts-node-dev": "1.0.0-pre.43",
    "typescript": "3.6.4",
    "uuid": "^3.3.3"
  }
}